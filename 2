#include <stdio.h>

int main() {
    FILE *f1 = fopen("input.txt", "r");
    if (f1 == NULL) {
        printf("Ошибка открытия файла input.txt\n");
        return 1;
    }

    FILE *f2 = fopen("output.txt", "w");
    if (f2 == NULL) {
        printf("Ошибка открытия файла output.txt\n");
        fclose(f2);
        return 1;
    }

    int num;
    while (fscanf(f1, "%d", &num) == 1) {
        if (num % 5 == 0) {
            fprintf(f2, "ПЯТЬ\n");
        } else {
            fprintf(f2, "%d\n", num);
        }
    }

    fclose(f1);
    fclose(f2);

    return 0;
}
